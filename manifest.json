{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Bubble",
    "version": "1.0.0",
    "minimum-overwolf-version": "0.199.0.15",
    "author": "Winston Co",
    "icon": "Clueless.png",
    "icon_gray": "TrollDespair.png",
    "description": "Demo App"
  },
  "data": {
    "start_window": "MainWindow",
    "windows": {
      "MainWindow": {
        "file": "src/index.html",
        "transparent": false,
        "resizable": true,
        "use_os_windowing": true,
        "size": {
          "width": 700,
          "height": 400
        },
        "min_size": {
          "width": 400,
          "height": 400
        }
      }
    }
  },
  "developer": {
    "enable_auto_refresh": true
  }
}
